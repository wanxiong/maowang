.nav{
  width: 100%;
  overflow: hidden;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1020;
}
.nonav{
  width: 100%;
  /* overflow: hidden; */
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
}
.nav-input{
  margin-left: 30rpx;
  width:496rpx;
  height:32px;
  border-radius:32rpx;
  position: absolute;
  background:rgba(0,0,0,.1);
}
.nav-input-ok{
  background-color:#F2F0F1;

}
.nav-qr{
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  left: 26rpx;
  top: 50%;
  transform: translateY(-50%);
}
.nav-qr-box{
  width: 60rpx;
  height: 100%;
  position: absolute;
  left: 0;
  top: 50%;
  transform: translateY(-50%);
  z-index: 120;
}
.nav-search{
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  left: 94rpx;
  top: 50%;
  transform: translateY(-50%);
}
.nav-line{
  width: 3rpx;
  height: 30rpx;
  position: absolute;
  left: 76rpx;
  top: 50%;
  transform: translateY(-50%);
  background-color: #fff;
}
.nav-line-ok{
  width: 3rpx;
  height: 30rpx;
  position: absolute;
  left: 76rpx;
  top: 50%;
  transform: translateY(-50%);
  background-color: #999;
}
.nav-input input{
  height:32px;
  font-size: 26rpx;
  text-align: left;
  padding-left: 130rpx;
}
.placeholder-class{
   color: rgba(255, 255, 255, .8);
   font-size: 12px;
}
.placeholder-class-ok{
    color: #999;
   font-size: 12px;
}
.nav-title{
  width: 100%;
  height: 45px;
  line-height: 45px;
  text-align: center;
  position: relative;
  bottom: 0;
  left: 0;
  z-index: 10;
  font-family:PingFang-SC-Medium;
  letter-spacing:2px;
  font-size:40rpx;
}
.nav .back{
  width: 22px;
  height: 22px;
  position: absolute;
  bottom: 0;
  left: 0;
  padding: 10px 15px;
}
.bg-white{
  background-color: #ffffff;
}
.bg-gray{
  background-color: #f7f7f7;
}
.overflow{
  overflow: auto;
}
.nav-modal{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  opacity: 1;
  display: block;
  z-index: 99;
}
.save{
  position: absolute;
  bottom: -20rpx;
  right: 0;
  box-shadow:0px 36px 60px 0px rgba(0, 0, 0, 0.2);
  padding: 15rpx 70rpx 15rpx 20rpx;
  font-size:24rpx;
  color: #fff;
  display: inline-flex;
  background:rgba(0,0,0,.8);
  transform: translateY(100%);
  z-index: 999;
  line-height: 27rpx;
  border-radius: 27rpx;
  justify-content: space-between;
  align-items: center;
}
.save image{
  width: 20rpx;
  height: 20rpx;
}
.save .sanjiao{
  content: "";
  position: absolute;
  width: 0;
  height: 0;
  top: 0;
  border-width: 10px;
  border-style: solid;
  border-color: transparent transparent rgba(0,0,0,.8) transparent;
  transform: translateY(-100%);
}
.save view{
  position: absolute;
  padding: 10rpx;
  right: 15rpx;
  top: 50%;
  transform: translateY(-50%);
}